<PropertyList>
  <path>landingcone.ac</path>

 <animation>
  <type>select</type>
  <object-name>LandingCone</object-name>
  <condition>
   <greater-than>
    <property>controls/lighting/external/main-landing[0]/norm</property>
    <property n="1">/controls/lighting/external/interpolate/off</property>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LandingCone</object-name>
  <property>controls/lighting/external/main-landing[0]/norm</property>
  <interpolation>
   <entry>
     <ind>0.0</ind>
     <dep>-70.0</dep>
   </entry>
   <entry>
     <ind>0.4</ind>
     <dep>-70.0</dep>
   </entry>
   <entry>
     <ind>1.0</ind>
     <dep>0.0</dep>
   </entry>
  </interpolation>   
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0.16</z>
  </axis>
 </animation>

</PropertyList>

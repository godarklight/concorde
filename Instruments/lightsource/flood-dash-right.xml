<?xml version="1.0"?>

<!-- default quarter of sphere is aft down.

     Lights down dash panel, from top of dash panel.
-->

<PropertyList>

<path>flood_dash.ac</path>


 <animation>
  <type>light</type>
  <light-type>point</light-type>
  <name>LeftDashFlood</name>
  <object-name>QuarterSphere</object-name>
  <nopreview/>
  <position>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </position>
  <ambient>
   <r>0.03</r>
   <g>0.005</g>
   <b>0.005</b>
   <a>1.0</a>
  </ambient>
  <diffuse>
   <r>0.83</r>
   <g>0.74</g>
   <b>0.38</b>
   <a>1.0</a>
  </diffuse>
  <specular>
   <r>0.83</r>
   <g>0.74</g>
   <b>0.38</b>
   <a>1.0</a>
  </specular>
  <dim-factor>
   <property>controls/lighting/crew/copilot/flood-light</property>
   <!--expression /-->
   <!--interpolation /-->
   <factor>1</factor>
   <offset>0</offset>
   <min>0</min>
   <max>1</max>
  </dim-factor>
  <attenuation>
   <c>1.0</c>
   <l>0.002</l>
   <q>0.00005</q>
  </attenuation>
  <exponent>30.0</exponent>
  <cutoff>39</cutoff>
  <near-m>0.01</near-m>
  <far-m>0.36</far-m>
 </animation>

 <animation>
  <type>select</type>
  <object-name>QuarterSphere</object-name>
  <condition>
   <and>
    <property>/sim/rendering/rembrandt/enabled</property>
    <greater-than>
     <property>/sim/rendering/shaders/lights</property>
     <property n="1">controls/environment/rembrandt/panel</property>
    </greater-than>
   </and>
  </condition>
 </animation>

</PropertyList>


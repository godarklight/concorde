<?xml version="1.0"?>

<!-- Glass :

     - 3D indicator of virtual crew.
 
-->

<PropertyList>

<path>glass.ac</path>

 <animation>
  <type>pick</type>
  <object-name>Glass</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>globals.Concorde.crewscreen.minimizeexport();</script>
   </binding>
  </action>
 </animation>

</PropertyList>

